<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">DrawingResult<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Historial diario", "Index", "DrawingResults")</li>
                            <li>@Html.ActionLink("Historial semanal", "WeekResult", "DrawingResults")</li>
                            <li>@Html.ActionLink("Historial mensual", "Index", "DrawingResults")</li>
                            <li>@Html.ActionLink("Historial anual", "Index", "DrawingResults")</li>
                            <li>@Html.ActionLink("Create New", "Create", "DrawingResults")</li>

                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">AdvancedOperations<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Delay", "DelayOperation", "AdvancedOperations")</li>
                            <li>@Html.ActionLink("Delated Numbers", "DelatedNumbers", "AdvancedOperations")</li>
                            <li>@Html.ActionLink("Path Numbers", "PathNumbers", "AdvancedOperations")</li>


                        </ul>
                    </li>
                    <li>@Html.ActionLink("Simulation", "SimulationView", "Simulation")</li>
                    <li>@Html.ActionLink("Cabala", "Index", "Cabala")</li>


                    DelayOperation
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
